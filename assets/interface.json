{
    "name": "MAD",
    "url": "https://github.com/huanshang141/MadAssistantDuet",
    "mirrorchyan_rid": "MadAssistantDuet",
    "mirrorchyan_multiplatform": true,
    "mfa_max_version": "v1.7.1",
    "mfa_min_version": "v1.7.1",
    "controller": [
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "class_regex": ".*",
                "window_regex": "二重螺旋|Duet Night Abyss",
                "input": 4,
                "screencap": 16
            }
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        }
    ],
    "agent": {
        "child_exec": "{PROJECT_DIR}/agent/MaaAgent.exe",
        "child_args": []
    },
    "task": [
        {
            "name": "65级mod（扼守）",
            "entry": "set_dodge_key",
            "doc": "进入指定副本后，即可启动，会先执行通用设置与通用开局",
            "option": ["闪避键设置","自动战斗设置"],
            "advanced": ["单轮战斗超时","自动E周期"],
            "pipeline_override": {
                "set_round_timeout": { "next": ["common_entry"] },
                "common_start": { "next": ["def_map1_in_battle", "def_map1_in_battle_template", "common_start"] }
            }
        },
        {
            "name": "驱离通用",
            "entry": "set_dodge_key",
            
            "option": ["闪避键设置","自动战斗设置"],
            "advanced": ["单轮战斗超时","自动E周期"],
            "pipeline_override": {
                "set_round_timeout": { "next": ["common_entry"] },
                "common_start": { "next": ["expulsion_in_battle", "expulsion_in_battle_template", "common_start"] }
            }
        },
        {
            "name": "60级皎皎币",
            "entry": "set_dodge_key",
            
            "option": ["闪避键设置","自动战斗设置"],
            "advanced": ["单轮战斗超时","自动E周期","战斗轮数设置"],
            "pipeline_override": {
                "set_round_timeout": { "next": ["set_battle_rounds"] },
                "set_battle_rounds": { "next": ["common_entry"] },
                "common_start": { "next": ["JJcoin_in_battle", "common_start"] },
                "common_multi_rounds_battle": {
                    "recognition": "OCR",
                    "expected": ["保护探险家"],
                    "custom_action_param": {
                        "target_node": ["JJcoin_finish"],
                        "post_rounds": ["JJcoin_continue_1"]
                    },
                    "on_error": ["common_entry"],
                    "next": ["JJcoin_finish"]
                }
            }
        }
    ],
    "option": {
        "闪避键设置": {
            "cases": [
                {
                    "name": "左Shift键",
                    "pipeline_override": {
                        "set_dodge_key": {
                            "custom_action_param": {
                                "dodge_key": 160
                            }
                        }
                    }
                },
                {
                    "name": "Ctrl键",
                    "pipeline_override": {
                        "set_dodge_key": {
                            "custom_action_param": {
                                "dodge_key": 17
                            }
                        }
                    }
                },
                {
                    "name": "Alt键",
                    "pipeline_override": {
                        "set_dodge_key": {
                            "custom_action_param": {
                                "dodge_key": 18
                            }
                        }
                    }
                },
                {
                    "name": "空格键",
                    "pipeline_override": {
                        "set_dodge_key": {
                            "custom_action_param": {
                                "dodge_key": 32
                            }
                        }
                    }
                },
                {
                    "name": "数字5键",
                    "pipeline_override": {
                        "set_dodge_key": {
                            "custom_action_param": {
                                "dodge_key": 53
                            }
                        }
                    }
                },
                {
                    "name": "鼠标右键",
                    "pipeline_override": {
                        "set_dodge_key": {
                            "custom_action_param": {
                                "dodge_key": 2
                            }
                        }
                    }
                },
                {
                    "name": "鼠标侧键1",
                    "pipeline_override": {
                        "set_dodge_key": {
                            "custom_action_param": {
                                "dodge_key": 5
                            }
                        }
                    }
                },
                {
                    "name": "鼠标侧键2",
                    "pipeline_override": {
                        "set_dodge_key": {
                            "custom_action_param": {
                                "dodge_key": 6
                            }
                        }
                    }
                }
            ],
            "default_case": "左Shift键"
        },
        "自动战斗设置":{
            "cases": [
                {
                    "name": "循环E",
                    "pipeline_override": {
                        "set_auto_battle_mode":{
                            "custom_action_param":{
                                "auto_battle_mode": 0
                            }
                        }
                    }
                },
                                {
                    "name":"无操作",
                    "pipeline_override": {
                        "set_auto_battle_mode":{
                            "custom_action_param":{
                                "auto_battle_mode": 1
                            }
                        }
                    }
                }
            ],
            "default_case": "循环E"
        }
    }
    ,
    "advanced": {
        "战斗轮数设置": {
            "field": [
                "作战轮数"
            ],
            "type": [
                "int"
            ],
            "default": [
                "3"
            ],
            "pipeline_override": {
                "JJcoin_rounds": {
                    "custom_action_param": {
                        "battle_rounds": "{作战轮数}"
                    }
                }
            },
            "doc": "设置皎皎币副本的战斗轮数"
        },
        "单轮战斗超时":{
            "field": [
                "单轮战斗超时（毫秒）"
            ],
            "type": [
                "int"
            ],
            "default": [
                "200000"
            ],
            "pipeline_override": {
                "set_round_timeout": {
                    "custom_action_param": {
                        "round_timeout_ms": "{单轮战斗超时（毫秒）}"
                    }
                }
            },
            "doc": "设置皎皎币单轮战斗的超时时间，单位为毫秒"
        },
        "自动E周期": {
            "field": [
                "自动E周期（毫秒）"
            ],
            "type": [
                "int"
            ],
            "default": [
                "5000"
            ],
            "pipeline_override": {
                "set_auto_e_interval": {
                    "custom_action_param": {
                        "auto_e_interval_ms": "{自动E周期（毫秒）}"
                    }
                }
            },
            "doc": "设置自动战斗的 E 键触发周期（毫秒）"
        }

    }
}
